# 私有仓库，整合云信UI库和和缓医疗
source 'https://github.com/iOSCodingTogether/HHDocotrAndNIMKitSpecs.git'
# CocoaPods Source
source 'https://github.com/CocoaPods/Specs.git'

platform :ios, '9.0'
#use_frameworks!

target 'Doctor' do
    inhibit_all_warnings!
 	
    pod "AFNetworking"
    pod 'ReactiveCocoa', '~> 2.5.0'
    pod "Masonry"
    pod "MJRefresh"
    pod "SDWebImage"
    pod "MBProgressHUD"
    pod "MJExtension"
    pod "Toast"
#    pod "BRPickerView"
    pod "TZImagePickerController"
    pod "SDCycleScrollView"
    pod "IQKeyboardManager"
    pod 'JPFPSStatus', :configurations => ['Debug']
#    朋友圈用
    pod "MLLabel"
    pod "UUButton"
    # 和缓使用整合后的私有库
    pod "NIMKit/Full"

end


